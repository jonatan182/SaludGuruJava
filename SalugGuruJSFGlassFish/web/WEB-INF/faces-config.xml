<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <managed-bean>
        <managed-bean-name>cita</managed-bean-name>
        <managed-bean-class>com.beans.citasJSF</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>medico</managed-bean-name>
        <managed-bean-class>com.beans.medicoJSF</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <navigation-case>
            <from-outcome>citav</from-outcome>
            <to-view-id>/citas/citas.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>nuevacita</from-outcome>
            <to-view-id>/citas/nuevaCita.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>medicov</from-outcome>
            <to-view-id>/medico/medico.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>nuevoMedico</from-outcome>
            <to-view-id>/medico/ingresarMedico.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>consultaMedico</from-outcome>
            <to-view-id>/medico/consultaMedico.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>menu</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
